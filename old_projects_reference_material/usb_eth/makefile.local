# (check under device manager in Windows)
LPC21ISP_PORT = COM9
LPC21ISP_PORT = /dev/ttyUSB0

TARGET = test-usb-serial
TARGET = test-usb-mouse
TARGET = usb-eth

LDSCRIPT = lpc2148-ram
LDSCRIPT = lpc2148-flash

